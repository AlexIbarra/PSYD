 c100d90:	e2033001 	and	r3, r3, #1
 c100d94:	e20330ff 	and	r3, r3, #255	; 0xff
 c100d98:	e3530000 	cmp	r3, #0
 c100d9c:	1afffff8 	bne	c100d84 <uart0_putchar+0x1c>
	UTXH0 = ch;
 c100da0:	e59f3014 	ldr	r3, [pc, #20]	; c100dbc <uart0_putchar+0x54>
 c100da4:	e55b200d 	ldrb	r2, [fp, #-13]
 c100da8:	e5832000 	str	r2, [r3]
}
