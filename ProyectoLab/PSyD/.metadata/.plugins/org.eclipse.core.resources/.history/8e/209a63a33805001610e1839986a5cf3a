 c100de8:	e0823003 	add	r3, r2, r3
 c100dec:	e5d33000 	ldrb	r3, [r3]
 c100df0:	e1a00003 	mov	r0, r3
 c100df4:	ebffffdb 	bl	c100d68 <uart0_putchar>
		i++;
