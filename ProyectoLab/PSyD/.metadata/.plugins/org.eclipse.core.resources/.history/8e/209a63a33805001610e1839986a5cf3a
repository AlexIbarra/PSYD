int32 uart0_getint( void ){
	int32 num=0, signo=1;
	uint8 c = uart0_getchar();
	if(c == '-')
		signo = -1;
	while(c != '\n') {
 c1011fc:	e55b3015 	ldrb	r3, [fp, #-21]	; 0xffffffeb
 c101200:	e353000a 	cmp	r3, #10
 c101204:	1affffeb 	bne	c1011b8 <uart0_getint+0x44>
			num *= 10;
			num += c - 48;
		}
		c = uart0_getchar();
	}
	return num*signo;
