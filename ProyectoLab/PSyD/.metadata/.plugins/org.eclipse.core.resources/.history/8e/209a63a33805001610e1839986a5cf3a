 c100dd4:	e3a03000 	mov	r3, #0
 c100dd8:	e50b3010 	str	r3, [fp, #-16]
	while(s[i] != '\0'){
 c100ddc:	ea000008 	b	c100e04 <uart0_puts+0x44>
		uart0_putchar(s[i]);
 c100de0:	e51b3010 	ldr	r3, [fp, #-16]
