/*
** Enva una cadena de caracteres por la UART
*/
void uart0_puts( char *s ){
 c100dc0:	e1a0c00d 	mov	ip, sp
 c100dc4:	e92dd800 	push	{fp, ip, lr, pc}
 c100dc8:	e24cb004 	sub	fp, ip, #4
 c100dcc:	e24dd010 	sub	sp, sp, #16
 c100dd0:	e50b0018 	str	r0, [fp, #-24]	; 0xffffffe8
