uint32 uart0_gethex( void ){
	int32 num=0, val;
	uint8 c;

	c = uart0_getchar();
	while(c != '\n') {
 c101314:	e55b3011 	ldrb	r3, [fp, #-17]	; 0xffffffef
 c101318:	e353000a 	cmp	r3, #10
 c10131c:	1affffca 	bne	c10124c <uart0_gethex+0x28>
		num *= 16;
		c = uart0_getchar();
		if(c == '\n')
			num /= 16;
	}
	return num;
 c101320:	e51b3010 	ldr	r3, [fp, #-16]
}
